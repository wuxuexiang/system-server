<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.goktech.system.user.mapper.SysUserMapper">

	<select id="getUser" resultType="string" parameterType="long">
		SELECT
			login_name
		FROM
			sys_user
		WHERE
			id = #{id}
	</select>
</mapper>
